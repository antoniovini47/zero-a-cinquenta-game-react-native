{
  "cli": {
    "version": ">= 10.2.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ADMOB_APP_ID": "$EXPO_PUBLIC_ADMOB_APP_ID$",
        "EXPO_PUBLIC_ADMOB_BANNER_ID": "$EXPO_PUBLIC_ADMOB_BANNER_ID$",
        "EXPO_PUBLIC_ADMOB_INTERSTITAL_ID": "$EXPO_PUBLIC_ADMOB_INTERSTITAL_ID$"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
          "EXPO_PUBLIC_ADMOB_APP_ID": "$EXPO_PUBLIC_ADMOB_APP_ID$",
          "EXPO_PUBLIC_ADMOB_BANNER_ID": "$EXPO_PUBLIC_ADMOB_BANNER_ID$",
        "EXPO_PUBLIC_ADMOB_INTERSTITAL_ID": "$EXPO_PUBLIC_ADMOB_INTERSTITAL_ID$"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": "version"
      },
      "env": {
          "EXPO_PUBLIC_ADMOB_APP_ID": "$EXPO_PUBLIC_ADMOB_APP_ID$",
          "EXPO_PUBLIC_ADMOB_BANNER_ID": "$EXPO_PUBLIC_ADMOB_BANNER_ID$",
        "EXPO_PUBLIC_ADMOB_INTERSTITAL_ID": "$EXPO_PUBLIC_ADMOB_INTERSTITAL_ID$"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
